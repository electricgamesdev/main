<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence persistence_1_0.xsd"
	version="1.0">
	<persistence-unit name="H2O" transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<exclude-unlisted-classes>false</exclude-unlisted-classes>
		<class>com.hydrogen.model.Hydrides</class>
		<class>com.hydrogen.model.Source</class>
		<class>com.hydrogen.model.Component</class>
		<class>com.hydrogen.model.Workflow</class>
		<class>com.hydrogen.model.Entity</class>
		<class>com.hydrogen.model.Form</class>
		<class>com.hydrogen.model.Action</class>
		<class>com.hydrogen.model.Rules</class>
		<class>com.hydrogen.stage.Analytics</class>
		<class>com.hydrogen.stage.Dimension</class>
		<class>com.hydrogen.stage.Formating</class>
		<class>com.hydrogen.stage.Ingestion</class>
		<class>com.hydrogen.stage.Presentation</class>
		<class>com.hydrogen.stage.Validation</class>
		<properties>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver" />
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/H2O?serverTimezone=UTC" />
			<property name="javax.persistence.jdbc.user" value="root" />
			<property name="javax.persistence.jdbc.password" value="Star_4321" />
			<property name="eclipselink.ddl-generation" value="create-tables" />
			<property name="useUnicode" value="true" />
			<property name="useJDBCCompliantTimezoneShift" value="true" />
		
			<property name="eclipselink.ddl-generation.output-mode"
				value="database" />
		</properties>


	</persistence-unit>
</persistence>